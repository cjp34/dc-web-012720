<h1>Create a new user or <%= link_to "Login", login_path %></h1>

<%= render partial: "shared/error_list", locals: {new_object: @user}  %>

<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit %>
<% end %>